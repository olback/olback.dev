{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 8
    },
    "rules": {
        "semi": "error",
        "comma-spacing": "error",
        "no-constant-condition": "warn",
        "no-console": "warn"
    }
}
